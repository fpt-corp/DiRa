<launch>

	<include ns="bridge15" file="$(find rosbridge_server)/launch/rosbridge_websocket.launch">
		<param name="port" value="9015"/>
	</include>
	<include file="$(find astra_launch)/launch/astra.launch" />
	<node name="hal" pkg="hal" type="hal_node" output="screen"/>
	<node name="car_controller" pkg="car_controller" type="car_controller_node" output="screen" />
        <node name="team515" pkg="team515" type="team515_node" output="screen" args="set /camera/image/compressed format 'png'" />

</launch>
